<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
   <servlet>
        <servlet-name>servletActionManager</servlet-name>
        <servlet-class>com.mrwlogistica.servlet.ServletActionManager</servlet-class>
   </servlet>
   <servlet>
        <servlet-name>servletConfigManager</servlet-name>
        <servlet-class>com.mrwlogistica.servlet.ServletConfigManager</servlet-class>
   </servlet>
   <servlet>
        <servlet-name>servletConsulta</servlet-name>
        <servlet-class>com.mrwlogistica.servlet.servletConsulta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletLineasxCupon</servlet-name>
        <servlet-class>com.mrwlogistica.servlet.servletLineasxCupon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletAccionesxCupon</servlet-name>
        <servlet-class>com.mrwlogistica.servlet.servletAccionesxCupon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>arilgp</servlet-name>
        <servlet-class>com.mrwlogistica.aril.arilgp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletActionManager</servlet-name>
        <url-pattern>/exec.mrwl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletConfigManager</servlet-name>
        <url-pattern>/exec.config</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletConsulta</servlet-name>
        <url-pattern>/servletConsulta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletLineasxCupon</servlet-name>
        <url-pattern>/servletLineasxCupon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletAccionesxCupon</servlet-name>
        <url-pattern>/servletAccionesxCupon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>arilgp</servlet-name>
        <url-pattern>/arilgp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
	<welcome-file>
            index.jsp
        </welcome-file>
    </welcome-file-list>
    </web-app>
