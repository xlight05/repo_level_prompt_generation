body {
	background:#b5cefe none;
	color:#cd6d0e;
	font-family:verdana,tahoma,arial,verdana,sans-serif;
	font-size:11px;
}
p {
	margin:12px 0;
}
a {
	color:#00348f;
}
.key {
	text-decoration:underline;
}
input,
textarea {
	font-family:verdana,tahoma,arial,verdana,sans-serif;
	font-size:12px;
	padding:3px;
}
.qo-abs-position {
	position:absolute;
}
