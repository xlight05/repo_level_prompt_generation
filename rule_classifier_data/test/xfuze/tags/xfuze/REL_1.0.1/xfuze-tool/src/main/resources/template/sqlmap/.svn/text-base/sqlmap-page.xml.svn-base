<?xml version="1.0" encoding="UTF-8"?>

	<!-- ===========================================================================
	//   SELECT - PAGE - selectPageConjunctionAnywhere
	============================================================================ -->
	<select id="selectPageConjunctionAnywhere" parameterClass="${sqlmap.model.instanceName}" resultMap="${sqlmap.model.javaName}.get${sqlmap.model.javaName}Result">
		SELECT TOP $pageSize$ * FROM (
			SELECT TOP $pageSize$ * FROM (
				SELECT TOP $page$ * FROM ${sqlmap.tableName}
				<dynamic prepend=" WHERE ">
					<!-- EXACT MATCH -->
					<#list sqlmap.sqlMapFields as field>
					<#if field.exactMatch>
					<isNotNull prepend=" AND " property="${field.javaFieldName}">${field.databaseFieldName} = #${field.javaFieldName}#</isNotNull>
					</#if>
					</#list>
					<!-- LIKE MATCH -->
					<#list sqlmap.sqlMapFields as field>
					<#if field.likeMatch>
					<isNotEmpty prepend=" AND " property="${field.javaFieldName}">${field.databaseFieldName} LIKE #${field.javaFieldName},handler=${sqlmap.likeHandlerAlias}#</isNotEmpty>
					</#if>
					</#list>
				</dynamic>
				ORDER BY
					<#list sqlmap.primaryKeyFields as field>
					${field.databaseFieldName}<#if field_has_next>,</#if>
					</#list>
			) temp1	ORDER BY
				<#list sqlmap.primaryKeyFields as field>
				${field.databaseFieldName} DESC<#if field_has_next>,</#if>
				</#list>
		) temp2 ORDER BY
			<#list sqlmap.primaryKeyFields as field>
			${field.databaseFieldName}<#if field_has_next>,</#if>
			</#list>
	</select>

