package Entities;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class QC_BANG {
    private String MaQCBang = "";
    private String MaND = "";
    private String MaPhieuDangKy = "";
    private String MaGiaTien = "";


    public QC_BANG(){}
    public QC_BANG(String maQCBang, String maND, String maPhieuDK, String maGiaTien){
        this.MaQCBang = maQCBang;
        this.MaND = maND;
        this.MaPhieuDangKy = maPhieuDK;
        this.MaGiaTien = maGiaTien;
    }
    
    @XmlElement
    public String getMaQCBang() {
        return MaQCBang;
    }

    public void setMaQCBang(String MaQCBang) {
        this.MaQCBang = MaQCBang;
    }

    @XmlElement
    public String getMaND() {
        return MaND;
    }


    public void setMaND(String MaND) {
        this.MaND = MaND;
    }

    @XmlElement
    public String getMaPhieuDangKy() {
        return MaPhieuDangKy;
    }

    public void setMaPhieuDangKy(String MaPhieuDangKy) {
        this.MaPhieuDangKy = MaPhieuDangKy;
    }

    @XmlElement
    public String getMaGiaTien() {
        return MaGiaTien;
    }

    public void setMaGiaTien(String MaGiaTien) {
        this.MaGiaTien = MaGiaTien;
    }

}
