    //load dữ liệu cho combobox mã khách hàng
    private void LoadComBoBox(){
        try
        {
            ArrayList<KHACHHANG> lst = dbKhachHang.GetAllRows();
            for(int i=0;i<lst.size();i++)
            {
                KHACHHANG _k = lst.get(i);
                cbxMaKH.addItem(_k.getMaKH());
            }
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(this, e.toString());
        }
    }

