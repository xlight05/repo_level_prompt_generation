            dtm.addColumn(java.util.ResourceBundle.getBundle("Presentation/" + frmMain.resource).getString("cost"));
            dtm.addColumn(java.util.ResourceBundle.getBundle("Presentation/" + frmMain.resource).getString("address"));
            THONGKEDIAOC _t = null;
            for(int i=0;i<lst.size();i++)
            {
                Vector _v = new Vector();
                _t = lst.get(i);
                _v.add(i+1);
                _v.add(_t.getMaKH());
                _v.add(_t.getHoTen().trim());
                _v.add(_t.getMaDiaOC());
                _v.add(_t.getTrangThai());
                _v.add(_t.getGiaBan());
                String tenThanhPho = dbThanhPho.GetRowByID(_t.getDiaChiDiaOc().getMaThanhPho()).getTenThanhPho().trim();
                String tenQuan = dbQuan.GetRowByID(_t.getDiaChiDiaOc().getMaQuan()).getTenQuan().trim();
                String tenPhuong = dbPhuong.GetRowByID(_t.getDiaChiDiaOc().getMaPhuong()).getTenPhuong().trim();
                String tenDuong = dbDuong.GetRowByID(_t.getDiaChiDiaOc().getMaDuong()).getTenDuong().trim();
                String diaChiCT = _t.getDiaChiDiaOc().getDiaChiCT().trim();
                String diaChi = diaChiCT + " " + tenDuong + " " + tenPhuong + " " + tenQuan + " " + tenThanhPho;
                _v.add(diaChi);
                dtm.addRow(_v);
            }
            this.jTable1.setModel(dtm);
            this.jTable1.getColumnModel().getColumn(0).setPreferredWidth(20);
