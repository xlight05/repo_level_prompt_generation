package Entities;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;


@XmlRootElement
public class DIAOC {
    private String MaDiaOc = "";
    private String MaLoaiDiaOc = "";
    private DIACHI DiaChiDiaOc;
    private Float DienTichDat;
    private Float DienTichXayDung;
    private String Huong = "";
    private String ViTri = "";
    private String MoTa = "";
    private String TrangThai = "";
    private Float GiaBan;

    public DIAOC(){}
    public DIAOC(String maDiaOc, String maLoaiDiaOc, DIACHI diaChiDiaOc,
            Float dienTichDat, Float dienTichXayDung, String huong,
            String viTri, String moTa, String trangThai, Float giaBan){
            this.MaDiaOc = maDiaOc;
            this.MaLoaiDiaOc = maLoaiDiaOc;
            this.DiaChiDiaOc = diaChiDiaOc;
            this.DienTichDat = dienTichDat;
            this.DienTichXayDung = dienTichXayDung;
            this.Huong = huong;
            this.ViTri = viTri;
            this.MoTa = moTa;
            this.TrangThai = trangThai;
            GiaBan = giaBan;
    }
    
    @XmlElement
    public String getMaDiaOc() {
        return MaDiaOc;
    }

    public void setMaDiaOc(String MaDiaOc) {
        this.MaDiaOc = MaDiaOc;
    }

    @XmlElement
    public String getMaLoaiDiaOc() {
        return MaLoaiDiaOc;
    }

    public void setMaLoaiDiaOc(String MaLoaiDiaOc) {
        this.MaLoaiDiaOc = MaLoaiDiaOc;
    }

    @XmlElement
    public Float getDienTichDat() {
        return DienTichDat;
    }

    public void setDienTichDat(Float DienTichDat) {
        this.DienTichDat = DienTichDat;
    }

    @XmlElement
    public Float getDienTichXayDung() {
        return DienTichXayDung;
    }

    public void setDienTichXayDung(Float DienTichXayDung) {
        this.DienTichXayDung = DienTichXayDung;
    }

    @XmlElement
    public String getHuong() {
        return Huong;
    }

    public void setHuong(String Huong) {
        this.Huong = Huong;
    }

    @XmlElement
    public String getViTri() {
        return ViTri;
    }

    public void setViTri(String ViTri) {
        this.ViTri = ViTri;
    }

    @XmlElement
    public String getMoTa() {
        return MoTa;
    }

    public void setMoTa(String MoTa) {
        this.MoTa = MoTa;
    }

    @XmlElement
    public String getTrangThai() {
        return TrangThai;
    }

    public void setTrangThai(String TrangThai) {
        this.TrangThai = TrangThai;
    }

    @XmlElement
    public Float getGiaBan() {
        return GiaBan;
    }

    public void setGiaBan(Float GiaBan) {
        this.GiaBan = GiaBan;
    }

    @XmlElement
    public DIACHI getDiaChiDiaOc() {
        return DiaChiDiaOc;
    }

    public void setDiaChiDiaOc(DIACHI DiaChiDiaOc) {
        this.DiaChiDiaOc = DiaChiDiaOc;
    }

}
