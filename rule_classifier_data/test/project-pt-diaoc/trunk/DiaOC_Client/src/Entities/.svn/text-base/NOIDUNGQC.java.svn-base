package Entities;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class NOIDUNGQC {
    private String MaND = "";
    private String ThongTinDiaOc = "";
    private String MoTaTomTat = "";

    public NOIDUNGQC(){}
    public NOIDUNGQC(String maND, String thongTinDiaOc ,String moTaTomTat){
        this.MaND = maND;
        this.ThongTinDiaOc = thongTinDiaOc;
        this.MoTaTomTat = moTaTomTat;
    }
    
    @XmlElement
    public String getMaND() {
        return MaND;
    }

    public void setMaND(String MaND) {
        this.MaND = MaND;
    }

    @XmlElement
    public String getThongTinDiaOc() {
        return ThongTinDiaOc;
    }

    public void setThongTinDiaOc(String ThongTinDiaOc) {
        this.ThongTinDiaOc = ThongTinDiaOc;
    }

    @XmlElement
    public String getMoTaTomTat() {
        return MoTaTomTat;
    }

    public void setMoTaTomTat(String MoTaTomTat) {
        this.MoTaTomTat = MoTaTomTat;
    }
}
