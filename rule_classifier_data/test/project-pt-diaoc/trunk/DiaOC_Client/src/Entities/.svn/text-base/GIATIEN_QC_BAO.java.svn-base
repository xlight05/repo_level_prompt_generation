package Entities;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement
public class GIATIEN_QC_BAO {
    private String MaGiaTien = "";
    private String ViTri = "";
    private String KhoIn = "";
    private String MauSac = "";
    private String GhiChu = "";
    private Float GiaTien;

    public GIATIEN_QC_BAO(){}
    public GIATIEN_QC_BAO(String maGiaTien, String viTri, String khoIn, String mauSac, String ghiChu, Float giaTien){
        this.MaGiaTien = maGiaTien;
        this.ViTri = viTri;
        this.KhoIn = khoIn;
        this.MauSac = mauSac;
        this.GhiChu = ghiChu;
        this.GiaTien = giaTien;
    }
    
    @XmlElement
    public String getMaGiaTien() {
        return MaGiaTien;
    }

    public void setMaGiaTien(String MaGiaTien) {
        this.MaGiaTien = MaGiaTien;
    }

    @XmlElement
    public String getViTri() {
        return ViTri;
    }

    public void setViTri(String ViTri) {
        this.ViTri = ViTri;
    }

    @XmlElement
    public String getKhoIn() {
        return KhoIn;
    }

    public void setKhoIn(String KhoIn) {
        this.KhoIn = KhoIn;
    }

    @XmlElement
    public String getMauSac() {
        return MauSac;
    }

    public void setMauSac(String MauSac) {
        this.MauSac = MauSac;
    }

    @XmlElement
    public String getGhiChu() {
        return GhiChu;
    }

    public void setGhiChu(String GhiChu) {
        this.GhiChu = GhiChu;
    }

    @XmlElement
    public Float getGiaTien() {
        return GiaTien;
    }

    public void setGiaTien(Float GiaTien) {
        this.GiaTien = GiaTien;
    }
    
    @Override
    public String toString(){
        return this.getMaGiaTien();
    }

}
