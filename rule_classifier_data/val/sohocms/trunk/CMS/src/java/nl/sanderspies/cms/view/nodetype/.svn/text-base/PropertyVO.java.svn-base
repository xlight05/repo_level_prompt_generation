package nl.sanderspies.cms.view.nodetype;

/**
 *
 * @author Sander Spies
 */
public class PropertyVO {
    private String name;
    private int type;

    public void setName(String name){
        this.name = name;
    }
    
    public String getName(){
        return name;
    }
    
    public void setType(int type){
        this.type = type;
    }
    
    public int getType(){
        return type;
    }
}
