<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:c="http://java.sun.com/jstl/core">
<ui:composition template="/mainTemplate.xhtml">
	<ui:define name="content">
		<h:form>
			<h:outputLabel value="Fill registration form : " />
			<h:panelGrid columns="2" rendered="#{!userBean.authorised}">
				<h:outputLabel value="Login:" />
				<h:inputText required="true" value="#{registration.register.login}" />

				<h:outputLabel value="Password:" />
				<h:inputText required="true"
					value="#{registration.register.password}" />

			</h:panelGrid>
			<ui:include src="userDataToOrder.xhtml" />


			<h:commandButton value="Register"
				action="#{registration.doRegistration}" />
		</h:form>
	</ui:define>
</ui:composition>
</html>
