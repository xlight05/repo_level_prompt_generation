<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<!-- Data Sources -->
	<data-sources>
	</data-sources>

	<!-- Form Beans -->
	<form-beans>
		<form-bean name="webPage" type="net.homeip.tinwiki.web.forms.WebPage">
		</form-bean>
	</form-beans>

	<!-- Global Exceptions -->
	<global-exceptions>
	</global-exceptions>

	<!-- Global Forwards -->
	<global-forwards>
	</global-forwards>

	<!-- Action Mappings -->
	<action-mappings>
		<action name="webPage" path="/web/cmsEdit/editWebPageInput" scope="request" type="net.homeip.tinwiki.web.actions.cmsEdit.editWebPageInputAction">
			<forward name="success" path="/web/cmsEdit/editWebPageInput.jsp">
			</forward>
		</action>
		<action name="webPage" path="/web/cmsEdit/editWebPageConfirm" scope="request" type="net.homeip.tinwiki.web.actions.cmsEdit.editWebPageConfirmAction">
			<forward name="success" path="/web/cmsEdit/editWebPageConfirm.jsp">
			</forward>
		</action>
		<action name="webPage" path="/web/cmsEdit/editWebPageSubmit" scope="request" type="net.homeip.tinwiki.web.actions.cmsEdit.editWebPageSubmitAction">
			<forward name="success" path="/web/cmsEdit/editWebPageSubmit.jsp">
			</forward>
		</action>
		<action path="/web/addSection/addSection" type="net.homeip.tinwiki.web.actions.addSection.addSectionAction" name="webPage" scope="request">
		<forward name="success" path="/web/addSection/addSection.jsp">
		</forward>

		</action>
	</action-mappings>

	<!-- Message Resources -->
	<message-resources parameter="net.homeip.tinwiki.resources.ApplicationResources"/>

</struts-config>
