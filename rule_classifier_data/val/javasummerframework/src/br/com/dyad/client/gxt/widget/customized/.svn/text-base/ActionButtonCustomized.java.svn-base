package br.com.dyad.client.gxt.widget.customized;

import br.com.dyad.client.widget.ClientAction;

import com.extjs.gxt.ui.client.widget.button.Button;

public class ActionButtonCustomized extends Button{
	private ClientAction clientAction;

	public ActionButtonCustomized(ClientAction clientAction) {
		super();
		this.clientAction = clientAction;
	}

	public ClientAction getClientAction() {
		return clientAction;
	}

	public void setClientAction(ClientAction clientAction) {
		this.clientAction = clientAction;
	}
}
