.x-status-text {
	margin: 0;;
	font: normal 12px tahoma, arial, helvetica, sans-serif;
	white-space: nowrap;
	color: black !important;
}

.x-status-text-error {
	margin: 0;;
	font: normal 12px tahoma, arial, helvetica, sans-serif;
	white-space: nowrap;
	color: red !important;
}

.icon-wait {
  background: url(resources/images/default/wait.gif) no-repeat !important;
}

