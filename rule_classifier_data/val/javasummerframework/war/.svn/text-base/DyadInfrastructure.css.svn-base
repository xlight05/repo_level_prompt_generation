	background-image: url( resources/images/default/page_white_text.png ) !important;	
}


.x-info-tc {
  background: transparent url( resources/images/default/top-bottom.gif) repeat-x 0 0;
  overflow: hidden;
}
.x-info-tl {
  background: transparent url( resources/images/default/corners-sprite.gif) no-repeat 0 0;
  padding-left: 6px;
  zoom: 1;
}
.x-info-tr {
  background: transparent url( resources/images/default/corners-sprite.gif) no-repeat right 0;
  zoom: 1;
  padding-right: 6px;
}
.x-info-bc {
  background: transparent url( resources/images/default/top-bottom.gif) repeat-x 0 bottom;
  zoom: 1;
  font-size: 0px;
}
.x-info-bl {
  background: transparent url( resources/images/default/corners-sprite.gif) no-repeat 0 bottom;
  padding-left: 6px;
  zoom: 1;
}

.x-info-br {
  background: transparent url( resources/images/default/corners-sprite.gif) no-repeat right bottom;
  padding-right: 6px;
  zoom: 1;
}

