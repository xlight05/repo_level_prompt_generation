#container layers
layer.registry=classpath:META-INF/invoices/layers/services-registry.xml
layer.viewcontroller=classpath:META-INF/invoices/layers/mvc-layer.xml
#services-configuration

#core-services-log
log.service.type=log4j
log.service.aop.pointcut=execution(* org.jsemantic.servicerules.*.*(..))
