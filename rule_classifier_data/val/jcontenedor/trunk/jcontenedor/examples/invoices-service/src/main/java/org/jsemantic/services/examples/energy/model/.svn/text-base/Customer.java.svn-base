package org.jsemantic.services.examples.energy.model;

public class Customer {
	
	private String id = null;
	
	private String name = null;
	
	private String addres = null;
	
	private String nifOrCif = null;
	
	private String tariff = null;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAddres() {
		return addres;
	}

	public void setAddres(String addres) {
		this.addres = addres;
	}

	public String getNifOrCif() {
		return nifOrCif;
	}

	public void setNifOrCif(String nifOrCif) {
		this.nifOrCif = nifOrCif;
	}


	public String getTariff() {
		return tariff;
	}

	public void setTariff(String tariff) {
		this.tariff = tariff;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}
	
	public String toString() {
		return "Customer: " + id + "\nTariff: " + tariff;
	}
	
}
