<?xml version="1.0" encoding="UTF-8"?>
<project basedir=".." name="SolAgentPlatform">
    <property file="nbproject/nbjdk.properties"/>
    <property location="${netbeans.user}/build.properties" name="user.properties.file"/>
    <property file="${user.properties.file}"/>
    <import file="jdk.xml"/>
    <target depends="-jdk-init" name="host-compile">
        <ant inheritall="false" target="host-compile"/>
    </target>
    <target depends="-jdk-init" name="host-run">
        <ant inheritall="false" target="host-run"/>
    </target>
    <target depends="-jdk-init" name="clean">
        <ant inheritall="false" target="clean"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-nb">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-nb"/>
    </target>
    <target depends="-jdk-init,-jdk-presetdef-nbjpdastart" name="debug-fix">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="debug-fix"/>
    </target>
    <target depends="-jdk-init" name="run-selected-file-in-src">
        <ant antfile="nbproject/ide-file-targets.xml" inheritall="false" target="run-selected-file-in-src"/>
    </target>
</project>
