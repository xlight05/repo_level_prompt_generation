<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="pl.edu.agh.iosr.ftpserverremote.data.Server" table="Server">
        <id name="id" column="Id">
            <generator class="native"/>
        </id>
        <property name="hostname" column="Hostname" type="java.lang.String"/>
        <property name="port" column="Port" type="java.lang.Integer"/>
        <property name="serverId" column="serverid" type="java.lang.Long"/>
    </class>

</hibernate-mapping>
