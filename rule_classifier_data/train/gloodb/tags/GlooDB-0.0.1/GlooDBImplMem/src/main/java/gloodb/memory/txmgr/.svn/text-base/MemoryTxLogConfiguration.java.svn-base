package gloodb.memory.txmgr;

import gloodb.txmgr.TxLogConfiguration;

/**
 * Memory transaction log configuration.
 * 
 */
class MemoryTxLogConfiguration implements TxLogConfiguration {
    private final String nameSpace;
    
    public MemoryTxLogConfiguration(String nameSpace) {
        this.nameSpace = nameSpace;
    }

    public String getNameSpace() {
        return nameSpace;
    }
}
