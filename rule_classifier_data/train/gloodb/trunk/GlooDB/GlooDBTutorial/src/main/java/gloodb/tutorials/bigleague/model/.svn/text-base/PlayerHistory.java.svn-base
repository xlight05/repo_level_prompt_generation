package gloodb.tutorials.bigleague.model;

import java.io.Serializable;
import java.util.Date;

public abstract class PlayerHistory implements Serializable {
    private static final long serialVersionUID = -6421779777814608660L;

    private final Date date;

    public PlayerHistory(Date date) {
        super();
        this.date = date;
    }

    public Date getDate() {
        return date;
    }
}
