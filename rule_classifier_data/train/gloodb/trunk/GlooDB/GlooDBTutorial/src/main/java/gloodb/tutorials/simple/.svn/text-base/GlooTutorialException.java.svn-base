package gloodb.tutorials.simple;

/**
 * GlooDBTutorial runtime exception.
 */
public class GlooTutorialException extends RuntimeException {
	private static final long serialVersionUID = 2998727736859871123L;

	/**
	 * Default constructor.
	 */
	public GlooTutorialException() {
		super();
	}

	/**
	 * Creates an exception with a message and a cause.
	 * @param arg0 The message.
	 * @param arg1 The cause.
	 */
	public GlooTutorialException(String arg0, Throwable arg1) {
		super(arg0, arg1);
	}

	/**
	 * Creates an exception with a message.
	 * @param arg0 The message.
	 */
	public GlooTutorialException(String arg0) {
		super(arg0);
	}

	/**
	 * Creates an exception with a cause.
	 * @param arg0 The cause.
	 */
	public GlooTutorialException(Throwable arg0) {
		super(arg0);
	}
}
