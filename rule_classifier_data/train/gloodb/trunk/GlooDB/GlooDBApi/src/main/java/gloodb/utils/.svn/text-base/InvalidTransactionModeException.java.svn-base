package gloodb.utils;

import gloodb.GlooException;

/**
 * Throw when trying to use TxMode.Same with top level transaction.
 */
public class InvalidTransactionModeException extends GlooException {
	private static final long serialVersionUID = -6738812130453288191L;

	/**
	 * Creates an exception object with a message.
	 * @param message The message
	 */
	public InvalidTransactionModeException(String message) {
		super(message);
	}
	
}
