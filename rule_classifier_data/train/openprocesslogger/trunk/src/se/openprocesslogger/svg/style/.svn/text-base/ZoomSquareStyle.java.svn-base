package se.openprocesslogger.svg.style;

import java.io.Serializable;

public class ZoomSquareStyle implements Serializable {

	private double y0Offset = 0; // The zoom squares will appear on this are now in a separate iframe
	private double x0Offset = 70;
	
	// Zoom button properties
	private double buttonOffset = 10;
	private double buttonWidth = 20;
	private double overviewbuttonWidth = 40;
	private double buttonDist = 10;
	public static final int ZOOM_LEVELS = 10;
	private String zoomButtonsStyle = "fill:red;stroke:black;stroke-width:3;";
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -183667954218709629L;

	public double getY0Offset() {
		return y0Offset;
	}

	public void setY0Offset(double offset) {
		y0Offset = offset;
	}

	public double getX0Offset() {
		return x0Offset;
	}

	public void setX0Offset(double offset) {
		x0Offset = offset;
	}

	public double getButtonOffset() {
		return buttonOffset;
	}

	public void setButtonOffset(double buttonOffset) {
		this.buttonOffset = buttonOffset;
	}

	public double getButtonWidth() {
		return buttonWidth;
	}

	public void setButtonWidth(double buttonWidth) {
		this.buttonWidth = buttonWidth;
	}

	public double getButtonDist() {
		return buttonDist;
	}

	public void setButtonDist(double buttonDist) {
		this.buttonDist = buttonDist;
	}

	public String getZoomButtonsStyle() {
		return zoomButtonsStyle;
	}

	public void setZoomButtonsStyle(String zoomButtonsStyle) {
		this.zoomButtonsStyle = zoomButtonsStyle;
	}

	public void setOverviewbuttonWidth(double overviewbuttonWidth) {
		this.overviewbuttonWidth = overviewbuttonWidth;
	}

	public double getOverviewbuttonWidth() {
		return overviewbuttonWidth;
	}
	
	

	
}
