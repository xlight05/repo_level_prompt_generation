<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    
    <import resource="datamodel.xml" />
    
    <bean id="OpenDataExchangeContext" class="se.opendataexchange.controller.OpenDataExchangeController">
        <property name="lstControlSystems">
            <list>
                <ref bean="simulatedControllerContext"/>
            </list>
        </property>
    </bean>
    <bean id="simulatedControllerContext" class="se.opendataexchange.simulator.SimulationController">
        <property name="lstAddressSpaces">
            <list>
                <ref bean="simulatedAddressSpace"/>
            </list>
        </property>
    </bean>
</beans>
