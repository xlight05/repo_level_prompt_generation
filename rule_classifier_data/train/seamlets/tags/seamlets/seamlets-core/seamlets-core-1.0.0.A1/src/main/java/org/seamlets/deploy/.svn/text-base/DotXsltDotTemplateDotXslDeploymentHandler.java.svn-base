package org.seamlets.deploy;

import org.jboss.seam.deployment.AbstractDeploymentHandler;
import org.jboss.seam.deployment.DeploymentMetadata;

public class DotXsltDotTemplateDotXslDeploymentHandler extends AbstractDeploymentHandler {

	private static DeploymentMetadata	DOTXSLTDOTTRANSFORMERDOTXSL_SUFFIX_FILE_METADATA	= new DeploymentMetadata() {

																								public String getFileNameSuffix() {
																									return "xslt.template.xsl";
																								}
																							};

	public static final String			NAME												= "dotXsltDotTemplateDotXmlDeploymentHandler";

	public String getName() {
		return NAME;
	}

	public DeploymentMetadata getMetadata() {
		return DOTXSLTDOTTRANSFORMERDOTXSL_SUFFIX_FILE_METADATA;
	}

}